{
  "name": "ReactNativeTesterApp",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "start": "react-native start"
  },
  "dependencies": {
    "react": "16.0.0-alpha.12",
    "react-native": "^0.46.4"
  },
  "devDependencies": {
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "2.1.0",
    "eslint": "^4.5.0",
    "eslint-config-wix": "^1.1.16",
    "eslint-plugin-react-native": "^3.0.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "main": "index.js",
  "babel": {
    "env": {
      "specs": {
        "presets": [
          "react-native"
        ],
        "retainLines": "true"
      }
    }
  },
  "rnx": {
    "appName": "ReactNativeTesterApp",
    "iphoneModel": "iPhone 6s",
    "packageName": "com.org.ReactNativeTesterApp"
  },
  "detox": {
    "specs": "test/e2e",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/DerivedData/ReactNativeTesterApp/Build/Products/Debug-iphonesimulator/ReactNativeTesterApp.app",
        "type": "ios.simulator",
        "name": "iPhone 6s"
      },
      "ios.sim.release": {
        "binaryPath": "ios/DerivedData/ReactNativeTesterApp/Build/Products/Release-iphonesimulator/ReactNativeTesterApp.app",
        "type": "ios.simulator",
        "name": "iPhone 6s"
      }
    }
  }
}
